  <tei:fsdDecl>
    <tei:fsDecl type="transliteration-choice" xml:id="transliteration-choice">
      <tei:fDecl name="as-written">
        <tei:desc xml:lang="en">When something is transliterated, always show it as written in the original document.</tei:desc>
         <tei:vRange>
          <tei:vAlt>
            <tei:symbol value="YES"/>
            <tei:symbol value="NO"/>
          </tei:vAlt>
        </tei:vRange>
        <tei:vDefault>
          <tei:symbol value="NO"/>
        </tei:vDefault>
      </tei:fDecl>
      <tei:fDecl name="as-custom">
        <tei:desc xml:lang="en">When something is transliterated, always show it using the selected custom table.</tei:desc>
         <tei:vRange>
          <tei:vAlt>
            <tei:symbol value="YES"/>
            <tei:symbol value="NO"/>
          </tei:vAlt>
        </tei:vRange>
        <tei:vDefault>
          <tei:symbol value="YES"/>
        </tei:vDefault>
      </tei:fDecl>
    </tei:fsDecl>
  </tei:fsdDecl>
  <j:conditionGrp>
    <tei:fvLib>
      <tei:fs type="transliteration-choice" xml:id="set-as-written">
        <tei:f name="as-written"><tei:symbol value="YES"/></tei:f>
        <tei:f name="as-custom"><tei:symbol value="NO"/></tei:f>
      </tei:fs>
      <tei:fs type="transliteration-choice" xml:id="set-as-custom">
        <tei:f name="as-written"><tei:symbol value="NO"/></tei:f>
        <tei:f name="as-custom"><tei:symbol value="YES"/></tei:f>
      </tei:fs>
      <tei:fs type="transliteration-choice" xml:id="as-written-Y">
        <tei:f name="as-written"><tei:symbol value="YES"/></tei:f>
      </tei:fs>
      <tei:fs type="transliteration-choice" xml:id="as-written-N">
        <tei:f name="as-written"><tei:symbol value="NO"/></tei:f>
      </tei:fs>
      <tei:fs type="transliteration-choice" xml:id="as-custom-Y">
        <tei:f name="as-custom"><tei:symbol value="YES"/></tei:f>
      </tei:fs>
      <tei:fs type="transliteration-choice" xml:id="as-custom-N">
        <tei:f name="as-custom"><tei:symbol value="NO"/></tei:f>
      </tei:fs>
    </tei:fvLib>
  
  </j:conditionGrp>
