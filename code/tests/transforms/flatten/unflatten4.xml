<?xml version="1.0" encoding="UTF-8"?>
<tei:TEI xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:text>
        <j:concurrent xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0">
            <jf:merged xmlns:jf="http://jewishliturgy.org/ns/jlptei/flat/1.0">
                <div xmlns="http://www.tei-c.org/ns/1.0" jf:id="div1" jf:start="div1" jf:position="1" jf:relative="-1" jf:nchildren="-2" jf:nlevels="1" jf:nprecedents="1" jf:layer-id="div-layer"/>
                <jf:placeholder jf:id="seg1" jf:position="1" jf:relative="0" jf:nchildren="0" jf:nlevels="0" jf:nprecedents="0" jf:stream="stream"/>
                <ab xmlns="http://www.tei-c.org/ns/1.0" jf:id="ab1" jf:start="ab1" jf:position="2" jf:relative="-1" jf:nchildren="-2" jf:nlevels="1" jf:nprecedents="1" jf:layer-id="ab-layer"/>
                <jf:placeholder jf:id="seg2" jf:position="2" jf:relative="0" jf:nchildren="0" jf:nlevels="0" jf:nprecedents="0" jf:stream="stream"/>
                <div xmlns="http://www.tei-c.org/ns/1.0" jf:end="div1" jf:position="2" jf:relative="1" jf:nchildren="2" jf:nlevels="-1" jf:nprecedents="1" jf:layer-id="div-layer"/>
                <jf:placeholder jf:id="seg3" jf:position="3" jf:relative="0" jf:nchildren="0" jf:nlevels="0" jf:nprecedents="0" jf:stream="stream"/>
                <ab xmlns="http://www.tei-c.org/ns/1.0" jf:end="ab1" jf:position="3" jf:relative="1" jf:nchildren="2" jf:nlevels="-1" jf:nprecedents="1" jf:layer-id="ab-layer"/>
            </jf:merged>
            <j:streamText xml:id="stream">
                <tei:seg xml:id="seg1">one</tei:seg>
                <tei:seg xml:id="seg2">two</tei:seg>
                <tei:seg xml:id="seg3">three</tei:seg>
            </j:streamText>
        </j:concurrent>
    </tei:text>
</tei:TEI>
