<?xml version="1.0" encoding="UTF-8"?>
<tei:TEI xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:text>
        <j:concurrent xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0">
            <jf:unflattened xmlns:jf="http://jewishliturgy.org/ns/jlptei/flat/1.0">
                <tei:div jf:id="div1" jf:layer-id="div-layer">
                  <jf:placeholder jf:id="seg1" jf:stream="stream"/>
                  <tei:ab jf:id="ab1" jf:part="ab1" jf:layer-id="ab-layer">
                    <jf:placeholder jf:id="seg2" jf:stream="stream"/>
                  </tei:ab>
                </tei:div>
                <tei:ab jf:part="ab1" jf:layer-id="ab-layer">
                  <jf:placeholder jf:id="seg3" jf:stream="stream"/>
                </tei:ab>
            </jf:unflattened>
            <j:streamText xml:id="stream">
                <tei:seg xml:id="seg1">one</tei:seg>
                <tei:seg xml:id="seg2">two</tei:seg>
                <tei:seg xml:id="seg3">three</tei:seg>
            </j:streamText>
        </j:concurrent>
    </tei:text>
</tei:TEI>
